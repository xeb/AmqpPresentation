<?xml version="1.0"?>
<configuration>
  <system.web>
    <httpHandlers>
      <clear />
      <add path="*" verb="*" type="Nancy.Hosting.Aspnet.NancyHttpRequestHandler" />
    </httpHandlers>
  </system.web>
  <system.webServer>
    <handlers>
      <clear />
      <add name="Nancy" verb="*" type="Nancy.Hosting.Aspnet.NancyHttpRequestHandler" path="*" />
    </handlers>
  </system.webServer>
</configuration>